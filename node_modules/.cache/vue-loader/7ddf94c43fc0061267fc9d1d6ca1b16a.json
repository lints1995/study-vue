: {\n    CForm,\n    CFormItem,\n    CInput,\n  },\n  data() {\n    return {\n      userInfo: {\n        userName: \"\",\n        passWord: \"\",\n      },\n      rules: {\n        userName: [\n          {\n            required: true,\n            message: \"请输入用户名\",\n          },\n        ],\n        passWord: [\n          {\n            required: true,\n            message: \"请输入密码\",\n          },\n        ],\n      },\n    };\n  },\n  methods: {\n    login() {\n      this.$refs[\"loginForm\"].validate((res) => {\n        if (res) {\n          alert(\"验证通过\");\n        } else {\n          alert(\"验证失败\");\n        }\n      });\n    },\n  },\n};\n</script>\n"]}]}